{
  "name": "s3node",
  "version": "0.1.0",
  "description": "A JS runtime that serves .zap scripts from S3",
  "main": "dist/handler.js",
  "bin": {
    "zap": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/dev.ts",
    "init": "tsx src/cli.ts init"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.1000.0",
    "@aws-sdk/client-iam": "^3.1000.0",
    "@aws-sdk/client-lambda": "^3.1000.0",
    "@aws-sdk/client-s3": "^3",
    "@aws-sdk/lib-dynamodb": "^3.1000.0",
    "commander": "^14.0.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8",
    "@types/node": "^22",
    "tsx": "^4",
    "typescript": "^5"
  },
  "private": true
}
